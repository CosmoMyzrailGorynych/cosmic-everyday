@keyframes zoomInUp
    from
        opacity 0
        transform scale3d(0.1, 0.1, 0.1) translate3d(0, 3rem, 0)
    30%
        opacity 1
        transform scale3d(0.475, 0.475, 0.475) translate3d(0, -60px, 0)
.animated
    animation-duration 0.5s
    animation-fill-mode both

for i in zoomInUp
    .{i}
        @extends .animated
        animation-name i
